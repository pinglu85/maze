{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/jsx-uses-vars": "error",
      "no-unused-vars": [
        "error",
        {
            "varsIgnorePattern": "^createElement$"
        }
      ]
    }
}
